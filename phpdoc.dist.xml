<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor configVersion="3"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://www.phpdoc.org" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/phpDocumentor/phpDocumentor/master/data/xsd/phpdoc.xsd">
    <paths>
        <output>doc</output>
        <cache>doc/cache</cache>
    </paths>
    <version number="3.0">
        <api>
            <source dsn=".">
                <path>src</path>
            </source>
        </api>
    </version>
    <setting name="template.color" value="deep-purple" />
</phpdocumentor>
